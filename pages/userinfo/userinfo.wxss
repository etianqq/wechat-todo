.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-bottom: 20px;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.userInfo-wrapper {
    padding: 0 20px 20px;
}

.userInfo-wrapper label {
  font-size:28rpx;
  color:#9e9c9c;
  margin-bottom:20px;
}

.content {
  margin-top: 10px;
}